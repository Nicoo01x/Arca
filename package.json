{
  "name": "@nicoo01x/arca-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for ARCA (formerly AFIP) web services with WSAA authentication and electronic invoicing.",
  "author": "NicolÃ¡s Cabanillas",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx,js,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "arca",
    "afip",
    "wsaa",
    "wsfe",
    "sdk",
    "tax",
    "argentina",
    "soap",
    "invoice"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.6.7",
    "fast-xml-parser": "^4.3.2",
    "node-forge": "^1.3.1",
    "soap": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/node-forge": "^1.3.10",
    "@types/soap": "^0.21.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
